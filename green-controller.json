{
   "kind":"ReplicationController",
   "apiVersion":"v1",
   "metadata":{
      "name":"srv-finalpjdevops",
      "labels":{
         "app":"finalpjdevops"
      }
   },
   "spec":{
      "replicas":1,
      "selector":{
         "app":"finalpjdevops"
      },
      "template":{
         "metadata":{
            "labels":{
               "app":"finalpjdevops"
            }
         },
         "spec":{
            "containers":[
               {
                  "name":"finalpjdevops",
                  "image":"anandatreya/finalpjdevops",
                  "ports":[
                     {
                        "name":"http-server",
                        "containerPort":8000
                     }
                  ]
               }
            ]
         }
      }
   }
}